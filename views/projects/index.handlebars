{{!-- loop through projects because they are being returned as an array --}} {{#each projects}}
<div class="card card-body mb-2">
    <h4>{{title}}</h4>
    <p>{{details}}</p>
    <a class="btn btn-dark btn-block" href="/projects/edit/{{id}}">Edit</a>
</div>
{{!-- what to say if there are no projects entered --}} {{else}}
<p>No projects listed</p>
{{/each}}